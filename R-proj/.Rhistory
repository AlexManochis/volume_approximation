P = gen_cube(100, 'H')
num_of_points = 1000
plot_list = list()
for (i in c(1, seq(10, 100, by = 10)))
{
points = sample_points(P, n=num_of_points,
random_walk = list("walk" = "BiW", "walk_length" = i))
projected_points <- data.frame( x=points[1,], y=points[2,] )
plot = ggplot(projected_points, aes(x=x, y=y))+geom_point()
plot_list[[i]] = plot
}
